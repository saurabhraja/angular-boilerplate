pipeline{
  agent any
  tools {
    git 'localGIT'
    jdk 'localJDK'
    nodejs 'localNode'
  }
  stages{
    stage ('Code Checkout'){
      steps{
        echo "AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAa"
        checkout scm
      }
    }
    stage ('Install Dependencies'){
      steps{
        npm install
      }
    }
    stage ('Console Log'){
      steps{
        sh '''
          echo 'Hey there'
        '''
      }
    }
  }
}